    "Ros McMahon <rosmcmahon@gmail.com>"
  ],
  "license": "LGPL-2.1-or-later",
  "browserlist": "defaults",
  "dependencies": {
    "grommet": "^2.28.0",
    "grommet-icons": "^4.8.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-syntax-highlighter": "^15.5.0"
  },
  "devDependencies": {
    "@types/node": "^18.11.17",
    "@types/react": "^18.0.26",
    "@types/react-dom": "^18.0.10",
    "@types/react-syntax-highlighter": "^15.5.5",
    "chai": "latest",
    "mocha": "latest",
    "parcel": "latest",
    "process": "^0.11.10",
    "shx": "latest",
    "typescript": "latest"
  }
